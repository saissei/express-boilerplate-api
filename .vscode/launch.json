{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "vscode-jest-tests",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/jest/node_modules/.bin/jest",
      "protocol": "inspector",
      "args": ["--runInBand"],
      "windows": {
        "program": "${workspaceRoot}/node_modules/jest/node_modules/.bin/jest"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
